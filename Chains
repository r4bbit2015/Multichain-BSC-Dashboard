sum(case "toChainID" when 25 then 1 else 0 end) as "Cronos Mainnet",
sum(case "toChainID" when 40 then 1 else 0 end) as "Telos EVM Mainnet",
sum(case "toChainID"  when 1313161554 then 1 else 0 end) as "Aurora Mainnet",
sum(case "toChainID" when 288 then 1 else 0 end) as "Boba Network",
sum(case "toChainID" when 42220 then 1 else 0 end) as "Celo Mainnet",
sum(case "toChainID" when 1 then 1 else 0 end) as "Ethereum Mainnet"


with data As (select 
sum(case "toChainID" when 1 then 1 else 0 end) As EthereumMainnet,
sum(case "toChainID" when 10 then 1 else 0 end) As Optimism,
sum(case "toChainID"  when 25 then 1 else 0 end) As CronosMainnetBeta,
sum(case "toChainID" when 56 then 1 else 0 end) As BSC,
sum(case "toChainID" when 66 then 1 else 0 end) As OKXChainMainnet,
sum(case "toChainID" when 137 then 1 else 0 end) As PolygonMainnet,
sum(case "toChainID" when 199 then 1 else 0 end) As BitTorrentChainMainnet,
sum(case "toChainID" when 250 then 1 else 0 end) As FantomOpera,
sum(case "toChainID" when 288 then 1 else 0 end) As BobaNetwork,
sum(case "toChainID" when 321 then 1 else 0 end) As KCCMainnet,
sum(case "toChainID" when 1284 then 1 else 0 end) As Moonbeam,
sum(case "toChainID" when 1285 then 1 else 0 end) As Moonriver,
sum(case "toChainID" when 42161 then 1 else 0 end) As ArbitrumOne,
sum(case "toChainID" when 43114 then 1 else 0 end) As AvalancheCChain
from multichain_v3."AnyswapV4RouterStableCoin_evt_LogAnySwapOut" ) 

select Optimism from data 

